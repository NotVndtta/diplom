<div id="<%= dom_id experience %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Start date at:</strong>
    <%= experience.start_date_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">End date at:</strong>
    <%= experience.end_date_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= experience.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= experience.company %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= experience.user_id %>
  </div>
</div>
